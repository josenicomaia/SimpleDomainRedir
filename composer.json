{
    "name": "josenicomaia/simpledomainredir",
    "description": "Domain Redirect made Simple.",
    "keywords": [
        "domain",
        "redirect",
        "simple"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2",
        "pear/net_dns2": "^1.4",
        "layershifter/tld-extract": "^1.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "SimpleDomainRedir\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    }
}
